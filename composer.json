{
    "name": "jlekowski/battleships-apiclient",
    "description" : "Battleships (sea battle) game - API Client",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BattleshipsApi\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^6.2",
        "symfony/console": "^3.0",
        "symfony/event-dispatcher": "^3.1",
        "symfony/options-resolver": "^3.1",
        "psr/log": "^1.0",
        "psr/http-message": "^1.0",
        "symfony/stopwatch": "^3.1",
        "donatj/cli-toolkit": "^0.1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "2.*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "bin": [
        "bin/console"
    ]
}
